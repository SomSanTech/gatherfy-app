{
  "expo": {
    "name": "Gathefy",
    "slug": "gatherfy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gatherfy",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "useCleartextTraffic": true,
      "package": "com.sedtabodee.gatherfy",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "sdkVersion": "52.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "newArchEnabled": true,
    "extra": {
      "apiBaseUrl": "http://cp24us1.sit.kmutt.ac.th:8080",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfa809f4-13dd-4c4a-bf54-175cde940b3b"
      }
    }
  }
}
